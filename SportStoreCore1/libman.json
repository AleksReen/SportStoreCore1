{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.0.0-alpha.6",
      "destination": "wwwroot/twitter-bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.css",
        "css/bootstrap-grid.css",
        "css/bootstrap-reboot.css"
      ]
    },
    {
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot/font-awesome/"
    },
    {
      "library": "jquery@3.3.1",
      "destination": "wwwroot/jquery/",
      "files": [
        "jquery.js"
      ]
    },
    {
      "library": "jquery-validate@1.19.0",
      "destination": "wwwroot/jquery-validate/",
      "files": [
        "jquery.validate.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.js"
      ]
    }
  ]
}